loopback_users.guest = false
listeners.ssl.default = 5671
ssl_options.cacertfile           = /etc/rabbitmq/ca_certificate.pem
ssl_options.certfile             = /etc/rabbitmq/rabbit_certificate.pem
ssl_options.keyfile              = /etc/rabbitmq/rabbit_key.pem
#ssl_options.verify               = verify_peer
#ssl_options.fail_if_no_peer_cert = true
ssl_options.password = rabbit

management.listener.port = 15671
management.listener.ssl = true
management.listener.ssl_opts.cacertfile = /etc/rabbitmq/ca_certificate.pem
management.listener.ssl_opts.certfile = /etc/rabbitmq/rabbit_certificate.pem
management.listener.ssl_opts.keyfile = /etc/rabbitmq/rabbit_key.pem
management.ssl.password = rabbit